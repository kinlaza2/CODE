                        if [ $var -gt $tknRedeemReqQueueDLQ_MessageCount_throttling_value_warning  ]
                        then
                                if [ $var -gt $tknRedeemReqQueueDLQ_MessageCount_throttling_value_critical ]
                                then
                                echo -e "Queue $1 - CRITICAL!! - $1 MessageCount is $var greater than $tknRedeemReqQueueDLQ_MessageCount_throttling_value_critical "
                                        exit 2
                                else
                                echo -e "Queue $1 - WARNING! - $1 MessageCount is $var greater than $tknRedeemReqQueueDLQ_MessageCount_throttling_value_warning"
                                        exit 1;
                                fi
                        else
                                echo -e "OK - $1 MessageCount is$var - throttling: $tknRedeemReqQueueDLQ_MessageCount_throttling_value_warning"
                                exit 0;
                        fi
                        ;;

                                                *)
                                echo -e "Queue $1 - CRITICAL!! - AN UNEXPECTED ERROR OCCURED."
                                exit 2;

                esac
                ;;








###############################################  billingOptoutQueue  #####################################################

        billingOptoutQueue)
                case  $2  in
                        MessageCount)
                        var=`cat "$SCRIPTS_HOME"/BILLINGOPTOUT  | awk {'print $3'}`
                        if [ $var -gt $billingOptoutQueue_MessageCount_throttling_value_warning  ]
                        then
                                if [ $var -gt $billingOptoutQueue_MessageCount_throttling_value_critical ]
                                then
                                echo -e "Queue $1 - CRITICAL!! - $1 MessageCount is $var greater than $billingOptoutQueue_MessageCount_throttling_value_critical "
                                        exit 2
                                else
                                echo -e "Queue $1 - WARNING! - $1 MessageCount is $var greater than $billingOptoutQueue_MessageCount_throttling_value_warning"
                                        exit 1;
                                fi
                        else
                                echo -e "OK - $1 MessageCount is $var - throttling: $billingOptoutQueue_MessageCount_throttling_value_warning"
                                exit 0;
                        fi
                        ;;

                        ConsumerCount)
                        var=`cat "$SCRIPTS_HOME"/BILLINGOPTOUT  | awk {'print $4'}`
                        if [ $var -eq $billingOptoutQueue_ConsumerCount_value ]
                        then
                                echo -e "$1 consumers are OK - value: $var"
                                exit 0;
                        else
                                echo -e "Queue $1 - CRITICAL!! - consumers are - NOT OK - value: $var - expected: $billingOptoutQueue_ConsumerCount_value"
                                exit 2;
                        fi
                        ;;

                        MessagesAdded)
                        file="$SCRIPTS_HOME"/BILLINGOPTOUT_PREVIOUS_"$billingOptoutQueue_MessagesAdded_previous_file_index"
                        current=`cat "$SCRIPTS_HOME"/BILLINGOPTOUT | awk {'print $2'}`
                        previous=`cat $file | awk {'print $2'}`
                        var=`expr $current - $previous`
                        if [ $var -ge $billingOptoutQueue_MessagesAdded_difference_value ]
                        then
                                copy  
                                echo -e "$1 MessagesAdded has difference from previous metric - difference: $var"
                                exit 0;
                        else
                                copy  
                                echo -e "Queue $1 - CRITICAL!! - MessagesAdded was not changed from the previous $billingOptoutQueue_MessagesAdded_previous_file_index check value: $current "
                                exit 2;
                        fi

                        ;;
                        *)
                                echo -e "Queue $1 - CRITICAL!! - AN UNEXPECTED ERROR OCCURED.111"
                                exit 2;

                esac
                ;;



        	billingOptoutQueueEXP)
                case  $2  in
                        MessageCount)
                        var=`cat "$SCRIPTS_HOME"/BILLINGOPTOUT  | awk {'print $7'}`
                        if [ $var -gt $billingOptoutQueueEXP_MessageCount_throttling_value_warning  ]
                        then
                                if [ $var -gt $billingOptoutQueueEXP_MessageCount_throttling_value_critical ]
                                then
                                echo -e "Queue $1 - CRITICAL!! - $1 MessageCount is $var greater than $billingOptoutQueueEXP_MessageCount_throttling_value_critical "
                                        exit 2
                                else
                                echo -e "Queue $1 - WARNING! - $1 MessageCount is $var greater than $billingOptoutQueueEXP_MessageCount_throttling_value_warning"
                                        exit 1;
                                fi
                        else
                                echo -e "OK - $1 MessageCount is $var - throttling: $billingOptoutQueueEXP_MessageCount_throttling_value_warning"
                                exit 0;
                        fi
                        ;;

                                                *)
                                echo -e "Queue $1 - CRITICAL!! - AN UNEXPECTED ERROR OCCURED."
                                exit 2;

                esac
                ;;



        	billingOptoutQueueDLQ)
                case  $2  in
                        MessageCount)
                        var=`cat "$SCRIPTS_HOME"/BILLINGOPTOUT  | awk {'print $11'}`
                        if [ $var -gt $billingOptoutQueueDLQ_MessageCount_throttling_value_warning  ]
                        then
                                if [ $var -gt $billingOptoutQueueDLQ_MessageCount_throttling_value_critical ]
                                then
                                echo -e "Queue $1 - CRITICAL!! - $1 MessageCount is $var greater than $billingOptoutQueueDLQ_MessageCount_throttling_value_critical "
                                        exit 2
                                else
                                echo -e "Queue $1 - WARNING! - $1 MessageCount is $var greater than $billingOptoutQueueDLQ_MessageCount_throttling_value_warning"
                                        exit 1;
                                fi
                        else
                                echo -e "OK - $1 MessageCount is$var - throttling: $billingOptoutQueueDLQ_MessageCount_throttling_value_warning"
                                exit 0;
                        fi
                        ;;

                                                *)
                                echo -e "Queue $1 - CRITICAL!! - AN UNEXPECTED ERROR OCCURED."
                                exit 2;

                esac
                ;;
































