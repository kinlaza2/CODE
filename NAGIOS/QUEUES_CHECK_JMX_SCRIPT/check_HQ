# $1 = queue name:  eventsEXP eventsDLQ mtsEXP mtsDLQ customerCareEXP customerCareDLQ mosEXP mosDLQ moPreprocessQueueEXP moPreprocessQueueDLQ  eventsQueue mtsQueue mosQueue  customerCareQueue moPreprocessQueue  creditQueue  creditDLQ creditEXP viettelCmdQueue viettelCmdQueueEXP viettelCmdQueueDLQ onLimitMoSmsQueue onLimitMoSmsQueueDLQ onLimitMoSmsQueueEXP 
# $2 = attribute: MessageCount ConsumerCount MessagesAdded


#!/bin/sh

SCRIPTS_HOME=/home/nagios/monitoring/libexec/CUSTOM/QUEUES_INFORMATION_VERSION2_LIVE_2_USER_PROMO_TWO/
STORAGE_HOME=/home/nagios/monitoring/libexec/CUSTOM/QUEUES_INFORMATION_VERSION2_LIVE_2_USER_PROMO_TWO

for i in creditQueue eventsQueue mosQueue tknRedeemReqQueue asyncBillingQueue billingOptoutQueue chargingQueue customerCareQueue dlrQueue mtsQueue onLimitMoSmsQueue mosEXP ivrProcessQueue moPreprocessQueue moPreprocessQueueEXP billingOptoutQueueEXP mtsEXP onLimitMoSmsQueueEXP asyncBillingQueueEXP chargingQueueEXP dlrQueueEXP tknRedeemReqQueueEXP ivrProcessEXP creditEXP customerCareEXP eventsEXP billingOptoutQueueDLQ onLimitMoSmsQueueDLQ tknRedeemReqQueueDLQ customerCareDLQ mosDLQ mtsDLQ chargingQueueDLQ dlrQueueDLQ moPreprocessQueueDLQ asyncBillingQueueDLQ eventsDLQ creditDLQ ivrProcessDLQ externalChargingQueue externalChargingQueueDLQ externalChargingQueueEXP
do
        for j in MessagesAdded MessageCount ConsumerCount
        do
                cat "$STORAGE_HOME"/RESULT_SPAWN | grep -A 2 $i | grep $j | tail -1 | awk -F"=" {'print $2'} | awk -F";" {'print $1'} > "$STORAGE_HOME"/AATEMP_"$i"_"$j"
        done
done
wait

for i in creditQueue eventsQueue mosQueue tknRedeemReqQueue asyncBillingQueue billingOptoutQueue chargingQueue customerCareQueue dlrQueue mtsQueue onLimitMoSmsQueue mosEXP ivrProcessQueue moPreprocessQueue moPreprocessQueueEXP billingOptoutQueueEXP mtsEXP onLimitMoSmsQueueEXP asyncBillingQueueEXP chargingQueueEXP dlrQueueEXP tknRedeemReqQueueEXP ivrProcessEXP creditEXP customerCareEXP eventsEXP billingOptoutQueueDLQ onLimitMoSmsQueueDLQ tknRedeemReqQueueDLQ customerCareDLQ mosDLQ mtsDLQ chargingQueueDLQ dlrQueueDLQ moPreprocessQueueDLQ asyncBillingQueueDLQ eventsDLQ creditDLQ ivrProcessDLQ externalChargingQueue externalChargingQueueDLQ externalChargingQueueEXP
do
echo -e `date +%d_%m_%y_%T`" "  `cat "$SCRIPTS_HOME"/AATEMP_"$i"_MessagesAdded` " " `cat "$SCRIPTS_HOME"/AATEMP_"$i"_MessageCount`  " "  `cat "$SCRIPTS_HOME"/AATEMP_"$i"_ConsumerCount`  > "$SCRIPTS_HOME"/"$i"_FINAL
done 



paste $SCRIPTS_HOME/creditQueue_FINAL $SCRIPTS_HOME/creditEXP_FINAL $SCRIPTS_HOME/creditDLQ_FINAL > $SCRIPTS_HOME/CREDIT
paste $SCRIPTS_HOME/eventsQueue_FINAL $SCRIPTS_HOME/eventsEXP_FINAL $SCRIPTS_HOME/eventsDLQ_FINAL > $SCRIPTS_HOME/EVENTS
paste $SCRIPTS_HOME/mosQueue_FINAL $SCRIPTS_HOME/mosEXP_FINAL $SCRIPTS_HOME/mosDLQ_FINAL > $SCRIPTS_HOME/MOS
paste $SCRIPTS_HOME/tknRedeemReqQueue_FINAL $SCRIPTS_HOME/tknRedeemReqQueueEXP_FINAL $SCRIPTS_HOME/tknRedeemReqQueueDLQ_FINAL > $SCRIPTS_HOME/TKNREDEEMREQUEUE
paste $SCRIPTS_HOME/asyncBillingQueue_FINAL $SCRIPTS_HOME/asyncBillingQueueEXP_FINAL $SCRIPTS_HOME/asyncBillingQueueDLQ_FINAL > $SCRIPTS_HOME/ASYNCBILLING
paste $SCRIPTS_HOME/billingOptoutQueue_FINAL $SCRIPTS_HOME/billingOptoutQueueEXP_FINAL $SCRIPTS_HOME/billingOptoutQueueDLQ_FINAL > $SCRIPTS_HOME/BILLINGOPTOUT
paste $SCRIPTS_HOME/chargingQueue_FINAL $SCRIPTS_HOME/chargingQueueEXP_FINAL $SCRIPTS_HOME/chargingQueueDLQ_FINAL > $SCRIPTS_HOME/CHARGING
paste $SCRIPTS_HOME/customerCareQueue_FINAL $SCRIPTS_HOME/customerCareEXP_FINAL $SCRIPTS_HOME/customerCareDLQ_FINAL > $SCRIPTS_HOME/CUSTOMER_CARE
paste $SCRIPTS_HOME/dlrQueue_FINAL $SCRIPTS_HOME/dlrQueueEXP_FINAL  $SCRIPTS_HOME/dlrQueueDLQ_FINAL >  $SCRIPTS_HOME/DLR
paste $SCRIPTS_HOME/mtsQueue_FINAL $SCRIPTS_HOME/mtsEXP_FINAL  $SCRIPTS_HOME/mtsDLQ_FINAL > $SCRIPTS_HOME/MTS
paste $SCRIPTS_HOME/ivrProcessQueue_FINAL $SCRIPTS_HOME/ivrProcessEXP_FINAL $SCRIPTS_HOME/ivrProcessDLQ_FINAL > $SCRIPTS_HOME/IVR
paste $SCRIPTS_HOME/onLimitMoSmsQueue_FINAL $SCRIPTS_HOME/onLimitMoSmsQueueEXP_FINAL  $SCRIPTS_HOME/onLimitMoSmsQueueDLQ_FINAL >  $SCRIPTS_HOME/ONLIMITMOSMS
paste $SCRIPTS_HOME/moPreprocessQueue_FINAL $SCRIPTS_HOME/moPreprocessQueueEXP_FINAL $SCRIPTS_HOME/moPreprocessQueueDLQ_FINAL > $SCRIPTS_HOME/MOPREPROCESS
paste $SCRIPTS_HOME/externalChargingQueue_FINAL  $SCRIPTS_HOME/externalChargingQueueEXP_FINAL $SCRIPTS_HOME/externalChargingQueueDLQ_FINAL > $SCRIPTS_HOME/EXTERNALCHARGING

wait


externalChargingQueue_ConsumerCount_value=10
externalChargingQueue_MessageCount_throttling_value_warning=2000
externalChargingQueue_MessageCount_throttling_value_critical=3000
externalChargingQueue_MessagesAdded_difference_value=1
externalChargingQueue_MessagesAdded_previous_file_index=$3


creditQueue_ConsumerCount_value=10
creditQueue_MessageCount_throttling_value_warning=2000
creditQueue_MessageCount_throttling_value_critical=3000
creditQueue_MessagesAdded_difference_value=1
creditQueue_MessagesAdded_previous_file_index=$3

eventsQueue_ConsumerCount_value=1
eventsQueue_MessageCount_throttling_value_warning=10000
eventsQueue_MessageCount_throttling_value_critical=30000
eventsQueue_MessagesAdded_difference_value=1
eventsQueue_MessagesAdded_previous_file_index=$3

mosQueue_ConsumerCount_value=10
mosQueue_MessageCount_throttling_value_warning=100
mosQueue_MessageCount_throttling_value_critical=500
mosQueue_MessagesAdded_difference_value=1
mosQueue_MessagesAdded_previous_file_index=$3

tknRedeemReqQueue_ConsumerCount_value=10
tknRedeemReqQueue_MessageCount_throttling_value_warning=100
tknRedeemReqQueue_MessageCount_throttling_value_critical=500
