dlrQueue_MessagesAdded_previous_file_index=$3

mtsQueue_ConsumerCount_value=7
mtsQueue_MessageCount_throttling_value_warning=100
mtsQueue_MessageCount_throttling_value_critical=1000
mtsQueue_MessagesAdded_difference_value=1
mtsQueue_MessagesAdded_previous_file_index=$3

ivrProcessQueue_ConsumerCount_value=6
ivrProcessQueue_MessageCount_throttling_value_warning=1000
ivrProcessQueue_MessageCount_throttling_value_critical=2500
ivrProcessQueue_MessagesAdded_difference_value=1
ivrProcessQueue_MessagesAdded_previous_file_index=$3

onLimitMoSmsQueue_ConsumerCount_value=10
onLimitMoSmsQueue_MessageCount_throttling_value_warning=2000
onLimitMoSmsQueue_MessageCount_throttling_value_critical=3000
onLimitMoSmsQueue_MessagesAdded_difference_value=1
onLimitMoSmsQueue_MessagesAdded_previous_file_index=$3

moPreprocessQueue_ConsumerCount_value=6
moPreprocessQueue_MessageCount_throttling_value_warning=1000
moPreprocessQueue_MessageCount_throttling_value_critical=2500
moPreprocessQueue_MessagesAdded_difference_value=1
moPreprocessQueue_MessagesAdded_previous_file_index=$3

tknRedeemReqQueueEXP_MessageCount_throttling_value_warning=1
tknRedeemReqQueueEXP_MessageCount_throttling_value_critical=5
tknRedeemReqQueueDLQ_MessageCount_throttling_value_warning=1
tknRedeemReqQueueDLQ_MessageCount_throttling_value_critical=5

asyncBillingQueueEXP_MessageCount_throttling_value_warning=1
asyncBillingQueueEXP_MessageCount_throttling_value_critical=5
asyncBillingQueueDLQ_MessageCount_throttling_value_warning=1
asyncBillingQueueDLQ_MessageCount_throttling_value_critical=5

billingOptoutQueueEXP_MessageCount_throttling_value_warning=1
billingOptoutQueueEXP_MessageCount_throttling_value_critical=5
billingOptoutQueueDLQ_MessageCount_throttling_value_warning=1
billingOptoutQueueDLQ_MessageCount_throttling_value_critical=5

chargingQueueEXP_MessageCount_throttling_value_warning=1
chargingQueueEXP_MessageCount_throttling_value_critical=5
chargingQueueDLQ_MessageCount_throttling_value_warning=1
chargingQueueDLQ_MessageCount_throttling_value_critical=5

dlrQueueEXP_MessageCount_throttling_value_warning=1
dlrQueueEXP_MessageCount_throttling_value_critical=5
dlrQueueDLQ_MessageCount_throttling_value_warning=1
dlrQueueDLQ_MessageCount_throttling_value_critical=5


eventsEXP_MessageCount_throttling_value_warning=1
eventsEXP_MessageCount_throttling_value_critical=5
eventsDLQ_MessageCount_throttling_value_warning=1
eventsDLQ_MessageCount_throttling_value_critical=5

creditEXP_MessageCount_throttling_value_warning=1
creditEXP_MessageCount_throttling_value_critical=5
creditDLQ_MessageCount_throttling_value_warning=20
creditDLQ_MessageCount_throttling_value_critical=50

mtsEXP_MessageCount_throttling_value_warning=1
mtsEXP_MessageCount_throttling_value_critical=5
mtsDLQ_MessageCount_throttling_value_warning=1
mtsDLQ_MessageCount_throttling_value_critical=5

customerCareEXP_MessageCount_throttling_value_warning=1
customerCareEXP_MessageCount_throttling_value_critical=5
customerCareDLQ_MessageCount_throttling_value_warning=2
customerCareDLQ_MessageCount_throttling_value_critical=5

mosEXP_MessageCount_throttling_value_warning=1
mosEXP_MessageCount_throttling_value_critical=5
mosDLQ_MessageCount_throttling_value_warning=1
mosDLQ_MessageCount_throttling_value_critical=5

moPreprocessQueueEXP_MessageCount_throttling_value_warning=1
moPreprocesQueueEXP_MessageCount_throttling_value_critical=5
moPreprocessQueueDLQ_MessageCount_throttling_value_warning=1
moPreprocessQueueDLQ_MessageCount_throttling_value_critical=5

ivrProcessEXP_MessageCount_throttling_value_warning=1
ivrProcessEXP_MessageCount_throttling_value_critical=5
ivrProcessDLQ_MessageCount_throttling_value_warning=1
ivrProcessDLQ_MessageCount_throttling_value_critical=5

onLimitMoSmsQueueEXP_MessageCount_throttling_value_warning=1
onLimitMoSmsQueueEXP_MessageCount_throttling_value_critical=5
onLimitMoSmsQueueDLQ_MessageCount_throttling_value_warning=1
onLimitMoSmsQueueDLQ_MessageCount_throttling_value_critical=5


externalChargingQueueEXP_MessageCount_throttling_value_warning=1
externalChargingQueueEXP_MessageCount_throttling_value_critical=5
externalChargingQueueDLQ_MessageCount_throttling_value_warning=1
externalChargingQueueDLQ_MessageCount_throttling_value_critical=5



function copy()
{
for i in MOS MTS EVENTS CUSTOMER_CARE CREDIT  IVR TKNREDEEMREQUEUE ASYNCBILLING BILLINGOPTOUT CHARGING DLR ONLIMITMOSMS MOPREPROCESS EXTERNALCHARGING
do
  cp -f  "$SCRIPTS_HOME"/"$i"_PREVIOUS_4 "$SCRIPTS_HOME"/"$i"_PREVIOUS_5
	cp -f "$SCRIPTS_HOME"/"$i"_PREVIOUS_3  "$SCRIPTS_HOME"/"$i"_PREVIOUS_4
	cp -f "$SCRIPTS_HOME"/"$i"_PREVIOUS_2  "$SCRIPTS_HOME"/"$i"_PREVIOUS_3
	cp -f "$SCRIPTS_HOME"/"$i"_PREVIOUS_1  "$SCRIPTS_HOME"/"$i"_PREVIOUS_2
	cp -f "$SCRIPTS_HOME"/$i  "$SCRIPTS_HOME"/"$i"_PREVIOUS_1
done
}



################# mosQueue ##############################
case  $1  in
	mosQueue)       
    		case  $2  in
			MessageCount)
                        var=`cat "$SCRIPTS_HOME"/MOS  | awk {'print $3'}`
                        if [ $var -gt $mosQueue_MessageCount_throttling_value_warning  ]
                        then
				if [ $var -gt $mosQueue_MessageCount_throttling_value_critical ]
				then
                                echo -e "Queue $1 - CRITICAL!! - $1 MessageCount is $var greater than $mosQueue_MessageCount_throttling_value_critical "
					exit 2
				else
				echo -e "Queue $1 - WARNING! - $1 MessageCount is $var greater than $mosQueue_MessageCount_throttling_value_warning"
					exit 1;
				fi
                        else
                                echo -e "OK - $1 MessageCount is $var - throttling: $mosQueue_MessageCount_throttling_value_warning"
				exit 0;
                        fi
			;;

                #######################fi
			ConsumerCount)
			var=`cat "$SCRIPTS_HOME"/MOS  | awk {'print $4'}`
			if [ $var -eq $mosQueue_ConsumerCount_value ]
			then
				echo -e "$1 consumers are OK - value: $var"
				exit 0;
			else
                        	echo -e "Queue $1 - CRITICAL!! - consumers are - NOT OK - value: $var - expected: $mosQueue_ConsumerCount_value"
                                exit 2;
			fi
			;;
			
			MessagesAdded)
			file="$SCRIPTS_HOME"/MOS_PREVIOUS_"$mosQueue_MessagesAdded_previous_file_index"
			current=`cat "$SCRIPTS_HOME"/MOS | awk {'print $2'}`
			previous=`cat $file | awk {'print $2'}`
			var=`expr $current - $previous`
			if [ $var -ge $mosQueue_MessagesAdded_difference_value ]
                        then
				copy  
                                echo -e "$1 MessagesAdded has difference from previous metric - difference: $var"
                                exit 0;
                        else
				copy  
                                echo -e "Queue $1 - CRITICAL!! - MessagesAdded was not changed from the previous $mosQueue_MessagesAdded_previous_file_index check value: $current "
                                exit 2;
                        fi

			;;
			*) 
				echo -e "Queue $1 - CRITICAL!! - AN UNEXPECTED ERROR OCCURED."
				exit 2;

		esac
		;;
                

        	mosEXP)
                case  $2  in
                        MessageCount)
                        var=`cat "$SCRIPTS_HOME"/MOS  | awk {'print $7'}`
                        if [ $var -gt $mosEXP_MessageCount_throttling_value_warning  ]
                        then
                                if [ $var -gt $mosEXP_MessageCount_throttling_value_critical ]
                                then
                                echo -e "Queue $1 - CRITICAL!! - $1 MessageCount is $var greater than $mosEXP_MessageCount_throttling_value_critical "
                                        exit 2
                                else
                                echo -e "Queue $1 - WARNING! - $1 MessageCount is $var greater than $mosEXP_MessageCount_throttling_value_warning"
                                        exit 1;
                                fi
                        else
                                echo -e "OK - $1 MessageCount is $var - throttling: $mosEXP_MessageCount_throttling_value_warning"
                                exit 0;
                        fi
                        ;;

                                                *)
                                echo -e "Queue $1 - CRITICAL!! - AN UNEXPECTED ERROR OCCURED."
                                exit 2;

                esac
                ;;



        	mosDLQ)
                case  $2  in
                        MessageCount)
                        var=`cat "$SCRIPTS_HOME"/MOS  | awk {'print $11'}`
                        if [ $var -gt $mosDLQ_MessageCount_throttling_value_warning  ]
                        then
                                if [ $var -gt $mosDLQ_MessageCount_throttling_value_critical ]
                                then
                                echo -e "Queue $1 - CRITICAL!! - $1 MessageCount is $var greater than $mosDLQ_MessageCount_throttling_value_critical "
                                        exit 2
                                else
                                echo -e "Queue $1 - WARNING! - $1 MessageCount is $var greater than $mosDLQ_MessageCount_throttling_value_warning"
                                        exit 1;
                                fi
                        else
                                echo -e "OK - $1 MessageCount is $var - throttling: $mosDLQ_MessageCount_throttling_value_warning"
                                exit 0;
                        fi
                        ;;

                                                *)
                                echo -e "Queue $1 - CRITICAL!! - AN UNEXPECTED ERROR OCCURED."
                                exit 2;

                esac
                ;;




########################################################### moPreprocessQueue ####################################
	moPreprocessQueue)       
		case  $2  in
			MessageCount)
                       	var=`cat "$SCRIPTS_HOME"/MOPREPROCESS  | awk {'print $3'}`
                        if [ $var -gt $moPreprocessQueue_MessageCount_throttling_value_warning  ]
                        	then
				if [ $var -gt $moPreprocessQueue_MessageCount_throttling_value_critical ]
				then
                                echo -e "Queue $1 - CRITICAL!! - $1 MessageCount is $var greater than $moPreprocessQueue_MessageCount_throttling_value_critical "
					exit 2
				else
				echo -e "Queue $1 - WARNING! - $1 MessageCount is $var greater than $moPreprocessQueue_MessageCount_throttling_value_warning"
					exit 1;
				fi
                        else
                                echo -e "OK - $1 MessageCount is $var - throttling: $moPreprocessQueue_MessageCount_throttling_value_warning"
				exit 0;
                        fi
			;;

			ConsumerCount)
			var=`cat "$SCRIPTS_HOME"/MOPREPROCESS  | awk {'print $4'}`
			if [ $var -eq $moPreprocessQueue_ConsumerCount_value ]
			then
				echo -e "$1 consumers are OK - value: $var"
				exit 0;
			else
                        	echo -e "Queue $1 - CRITICAL!! - consumers are - NOT OK - value: $var - expected: $moPreprocessQueue_ConsumerCount_value"
                                exit 2;
			fi
			;;
			
			MessagesAdded)
			file="$SCRIPTS_HOME"/MOPREPROCESS_PREVIOUS_"$moPreprocessQueue_MessagesAdded_previous_file_index"
			current=`cat "$SCRIPTS_HOME"/MOPREPROCESS | awk {'print $2'}`
			previous=`cat $file | awk {'print $2'}`
			var=`expr $current - $previous`
			if [ $var -ge $moPreprocessQueue_MessagesAdded_difference_value ]
                        then
				copy  
                                echo -e "$1 MessagesAdded has difference from previous metric - difference: $var"
                                exit 0;
                        else
				copy  
                                echo -e "Queue $1 - CRITICAL!! - MessagesAdded was not changed from the previous $moPreprocessQueue_MessagesAdded_previous_file_index check value: $current "
                                exit 2;
                        fi

			;;
			*) 
				echo -e "Queue $1 - CRITICAL!! - AN UNEXPECTED ERROR OCCURED."
				exit 2;

		esac
		;;


        	moPreprocessQueueEXP)
                case  $2  in
                        MessageCount)
                        var=`cat "$SCRIPTS_HOME"/MOPREPROCESS  | awk {'print $7'}`
                        if [ $var -gt $moPreprocessQueueEXP_MessageCount_throttling_value_warning  ]
                        then
                                if [ $var -gt $moPreprocessQueueEXP_MessageCount_throttling_value_critical ]
                                then
                                echo -e "Queue $1 - CRITICAL!! - $1 MessageCount is $var greater than $moPreprocessQueueEXP_MessageCount_throttling_value_critical "
                                        exit 2
                                else
                                echo -e "Queue $1 - WARNING! - $1 MessageCount is $var greater than $moPreprocessQueueEXP_MessageCount_throttling_value_warning"
                                        exit 1;
                                fi
                        else
                                echo -e "OK - $1 MessageCount is $var - throttling: $moPreprocessQueueEXP_MessageCount_throttling_value_warning"
                                exit 0;
                        fi
                        ;;

                                                *)
                                echo -e "Queue $1 - CRITICAL!! - AN UNEXPECTED ERROR OCCURED."
                                exit 2;

                esac
                ;;



        	moPreprocessQueueDLQ)
                case  $2  in
                        MessageCount)
                        var=`cat "$SCRIPTS_HOME"/MOPREPROCESS  | awk {'print $11'}`
                        if [ $var -gt $moPreprocessQueueDLQ_MessageCount_throttling_value_warning  ]
                        then
                                if [ $var -gt $moPreprocessQueueDLQ_MessageCount_throttling_value_critical ]
                                then
                                echo -e "Queue $1 - CRITICAL!! - $1 MessageCount is $var greater than $moPreprocessQueueDLQ_MessageCount_throttling_value_critical "
                                        exit 2
                                else
                                echo -e "Queue $1 - WARNING! - $1 MessageCount is $var greater than $moPreprocessQueueDLQ_MessageCount_throttling_value_warning"
                                        exit 1;
                                fi
                        else
                                echo -e "OK - $1 MessageCount is $var - throttling: $moPreprocessQueueDLQ_MessageCount_throttling_value_warning"
                                exit 0;
                        fi
                        ;;

                                                *)
                                echo -e "Queue $1 - CRITICAL!! - AN UNEXPECTED ERROR OCCURED."
                                exit 2;

                esac
                ;;








################################################################ ivrProcessQueue ############################################

	ivrProcessQueue)       
		case  $2  in
			MessageCount)
                       	var=`cat "$SCRIPTS_HOME"/IVR  | awk {'print $3'}`
                        if [ $var -gt $ivrProcessQueue_MessageCount_throttling_value_warning  ]
                        	then
				if [ $var -gt $ivrProcessQueue_MessageCount_throttling_value_critical ]
				then
                                echo -e "Queue $1 - CRITICAL!! - $1 MessageCount is $var greater than $ivrProcessQueue_MessageCount_throttling_value_critical "
					exit 2
				else
				echo -e "Queue $1 - WARNING! - $1 MessageCount is $var greater than $ivrProcessQueue_MessageCount_throttling_value_warning"
					exit 1;
				fi
                        else
                                echo -e "OK - $1 MessageCount is $var - throttling: $ivrProcessQueue_MessageCount_throttling_value_warning"
				exit 0;
                        fi
			;;

			ConsumerCount)
			var=`cat "$SCRIPTS_HOME"/IVR  | awk {'print $4'}`
			if [ $var -eq $ivrProcessQueue_ConsumerCount_value ]
			then
				echo -e "$1 consumers are OK - value: $var"
				exit 0;
			else
                        	echo -e "Queue $1 - CRITICAL!! - consumers are - NOT OK - value: $var - expected: $ivrProcessQueue_ConsumerCount_value"
                                exit 2;
			fi
			;;
			
			MessagesAdded)
			file="$SCRIPTS_HOME"/IVR_PREVIOUS_"$moPreprocessQueue_MessagesAdded_previous_file_index"
			current=`cat "$SCRIPTS_HOME"/IVR | awk {'print $2'}`
			previous=`cat $file | awk {'print $2'}`
			var=`expr $current - $previous`
			if [ $var -ge $ivrProcessQueue_MessagesAdded_difference_value ]
                        then
				copy  
                                echo -e "$1 MessagesAdded has difference from previous metric - difference: $var"
                                exit 0;
                        else
				copy  
                                echo -e "Queue $1 - CRITICAL!! - MessagesAdded was not changed from the previous $ivrProcessQueue_MessagesAdded_previous_file_index check value: $current "
                                exit 2;
                        fi

			;;
			*) 
				echo -e "Queue $1 - CRITICAL!! - AN UNEXPECTED ERROR OCCURED."
				exit 2;

		esac
		;;


        	ivrProcessEXP)
                case  $2  in
                        MessageCount)
                        var=`cat "$SCRIPTS_HOME"/IVR  | awk {'print $7'}`
                        if [ $var -gt $ivrProcessEXP_MessageCount_throttling_value_warning  ]
                        then
                                if [ $var -gt $ivrProcessEXP_MessageCount_throttling_value_critical ]
                                then
                                echo -e "Queue $1 - CRITICAL!! - $1 MessageCount is $var greater than $ivrProcessEXP_MessageCount_throttling_value_critical "
                                        exit 2
                                else
                                echo -e "Queue $1 - WARNING! - $1 MessageCount is $var greater than $ivrProcessEXP_MessageCount_throttling_value_warning"
                                        exit 1;
                                fi
                        else
                                echo -e "OK - $1 MessageCount is $var - throttling: $ivrProcessEXP_MessageCount_throttling_value_warning"
                                exit 0;
                        fi
                        ;;

                                                *)
                                echo -e "Queue $1 - CRITICAL!! - AN UNEXPECTED ERROR OCCURED."
                                exit 2;

                esac
                ;;



        	ivrProcessDLQ)
                case  $2  in
                        MessageCount)
                        var=`cat "$SCRIPTS_HOME"/IVR  | awk {'print $11'}`
                        if [ $var -gt $ivrProcessDLQ_MessageCount_throttling_value_warning  ]
                        then
                                if [ $var -gt $ivrProcessDLQ_MessageCount_throttling_value_critical ]
                                then
                                echo -e "Queue $1 - CRITICAL!! - $1 MessageCount is $var greater than $ivrProcessDLQ_MessageCount_throttling_value_critical "
                                        exit 2
                                else
                                echo -e "Queue $1 - WARNING! - $1 MessageCount is $var greater than $ivrProcessDLQ_MessageCount_throttling_value_warning"
                                        exit 1;
                                fi
                        else
                                echo -e "OK - $1 MessageCount is $var - throttling: $ivrProcessDLQ_MessageCount_throttling_value_warning"
                                exit 0;
                        fi
                        ;;

                                                *)
                                echo -e "Queue $1 - CRITICAL!! - AN UNEXPECTED ERROR OCCURED."
                                exit 2;

                esac
                ;;














######################################     creditQueue        ##################################################

      creditQueue)
                case  $2  in
                        MessageCount)
                        var=`cat "$SCRIPTS_HOME"/CREDIT  | awk {'print $3'}`
                        if [ $var -gt $creditQueue_MessageCount_throttling_value_warning  ]
                        then
                                if [ $var -gt $creditQueue_MessageCount_throttling_value_critical ]
                                then
                                echo -e "Queue $1 - CRITICAL!! - $1 MessageCount is $var greater than $creditQueue_MessageCount_throttling_value_critical "
                                        exit 2
                                else
                                echo -e "Queue $1 - WARNING! - $1 MessageCount is $var greater than $creditQueue_MessageCount_throttling_value_warning"
                                        exit 1;
                                fi
                        else
                                echo -e "OK - $1 MessageCount is $var - throttling: $creditQueue_MessageCount_throttling_value_warning"
                                exit 0;
                        fi
                        ;;

                        ConsumerCount)
                        var=`cat "$SCRIPTS_HOME"/CREDIT  | awk {'print $4'}`
                        if [ $var -eq $creditQueue_ConsumerCount_value ]
                        then
                                echo -e "$1 consumers are OK - value: $var"
                                exit 0;
                        else
                                echo -e "Queue $1 - CRITICAL!! - consumers are - NOT OK - value: $var - expected: $creditQueue_ConsumerCount_value"
                                exit 2;
                        fi
                        ;;

                        MessagesAdded)
                        file="$SCRIPTS_HOME"/CREDIT_PREVIOUS_"$creditQueue_MessagesAdded_previous_file_index"
                        current=`cat "$SCRIPTS_HOME"/CREDIT | awk {'print $2'}`
                        previous=`cat $file | awk {'print $2'}`
                        var=`expr $current - $previous`
                        if [ $var -ge $creditQueue_MessagesAdded_difference_value ]
                        then
                                copy
                                echo -e "$1 MessagesAdded has difference from previous metric - difference: $var"
                                exit 0;
                        else
                                copy
                                echo -e "Queue $1 - CRITICAL!! - MessagesAdded was not changed from the previous $creditQueue_MessagesAdded_previous_file_index check value: $current "
                                exit 2;
                        fi

                        ;;
                        *)
                                echo -e "Queue $1 - CRITICAL!! - AN UNEXPECTED ERROR OCCURED."
                                exit 2;

                esac
                ;;



                 creditEXP)
                case  $2  in
                        MessageCount)
                        var=`cat "$SCRIPTS_HOME"/CREDIT  | awk {'print $7'}`
                        if [ $var -gt $creditEXP_MessageCount_throttling_value_warning  ]
                        then
                                if [ $var -gt $creditEXP_MessageCount_throttling_value_critical ]
