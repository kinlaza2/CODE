asyncBillingQueue_MessageCount_throttling_value_critical=500
asyncBillingQueue_MessagesAdded_difference_value=1
asyncBillingQueue_MessagesAdded_previous_file_index=$3

billingOptoutQueue_ConsumerCount_value=10
billingOptoutQueue_MessageCount_throttling_value_warning=100
billingOptoutQueue_MessageCount_throttling_value_critical=500
billingOptoutQueue_MessagesAdded_difference_value=1
billingOptoutQueue_MessagesAdded_previous_file_index=$3

customerCareQueue_ConsumerCount_value=2
customerCareQueue_MessageCount_throttling_value_warning=100
customerCareQueue_MessageCount_throttling_value_critical=500
customerCareQueue_MessagesAdded_difference_value=1
customerCareQueue_MessagesAdded_previous_file_index=$3

dlrQueue_ConsumerCount_value=10
dlrQueue_MessageCount_throttling_value_warning=100
dlrQueue_MessageCount_throttling_value_critical=500
