                                echo -e "Queue $1 - CRITICAL!! - $1 MessageCount is $var greater than $creditEXP_MessageCount_throttling_value_critical "
                                        exit 2
                                else
                                echo -e "Queue $1 - WARNING! - $1 MessageCount is $var greater than $creditEXP_MessageCount_throttling_value_warning"
                                        exit 1;
                                fi
                        else
                                echo -e "OK - $1 MessageCount is $var - throttling: $creditEXP_MessageCount_throttling_value_warning"
                                exit 0;
                        fi
                        ;;

                                                *)
                                echo -e "Queue $1 - CRITICAL!! - AN UNEXPECTED ERROR OCCURED."
                                exit 2;

                esac
                ;;



                creditDLQ)
                case  $2  in
                        MessageCount)
                        var=`cat "$SCRIPTS_HOME"/CREDIT  | awk {'print $11'}`
                        if [ $var -gt $creditDLQ_MessageCount_throttling_value_warning ]
                        then
                                if [ $var -gt $creditDLQ_MessageCount_throttling_value_critical ]
